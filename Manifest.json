{
  "name": "Tab Query!", 
  "version": "1.0.0",
  "description": "Find your tab",
  "permissions": ["tabs","storage"],
  "manifest_version": 3,
  "author": "Osman Goni",
  "action":{
      "default_popup": "index.html",
      "default_title": "Latest Covid Report", 
      "default_icon": {              
        "16": "images/icon16.png",   
        "24": "images/icon24.png",   
        "32": "images/icon32.png"    
      }
  },
  "background": {
    "service_worker": "background.js"
  }
 



}